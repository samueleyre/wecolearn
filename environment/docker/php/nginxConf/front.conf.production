server {
    index index.html;
    server_name bg.xyzxyzxyzxyzxyzxyz.com;
    error_log  /var/log/nginx/error.log;
    access_log /var/log/nginx/access.log;
    root /src/front/dist;
    listen 443 ssl;

    #ssl_certificate /etc/letsencrypt/live/bg.xyzxyzxyzxyzxyzxyz.com/fullchain.pem;
    #ssl_certificate_key /etc/letsencrypt/live/bg.xyzxyzxyzxyzxyzxyz.com/privkey.pem;

    location ~* \.(js|jpg|jpeg|png|svg|gif|css|html|map|woff2|svg|ttf|eot|woff|json)$ {
        root /src/front/dist;
    }

}

server {
    listen 80;
    server_name bg.xyzxyzxyzxyzxyzxyz.com;
    location / {
        return 301 https://$host$uri;

    }
}